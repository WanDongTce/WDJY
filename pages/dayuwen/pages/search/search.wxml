<view class="search">
<input type='text' placeholder='搜索关键字' bindinput="searchval" name="searchval" bindconfirm='search'></input>
  <view class='btn' catchtap='search'>
    <text>搜索</text>
  </view>
</view>
<view class='History'>
  <view class='content_box'>
   <view class='title'>
      <view class='left'>
        <text>历史搜索</text>
      </view>
      <view class='right' catchtap='del'>
        <image src='/pages/images/del.png'></image>
      </view>
      <view class='clearfix'></view>
   </view>
   <view class='content'>
    <view class='list' wx:for="{{list}}" data-name='{{item}}' catchtap='tosearch'>
      <text>{{item}}</text>
    </view>
     
    <view class='clearfix'></view>
   </view>
  </view>
</view>